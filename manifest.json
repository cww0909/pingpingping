{
	"manifest_version": 2,
	"name": "pingpingping",
	"description": "Test your ping to a server over time with this extension",
	"version": "0.3",
	"app": {
		"background": {
			"scripts": ["main.js"]
		}
	},
	"icons": {
		"16": "ppp16.png",
		"32": "ppp32.png",
		"128": "ppp128.png"
	},
	"permissions": [
		"<all_urls>"
	]
}