{
	"manifest_version": 2,
	"name": "pingpingping",
	"description": "Test your ping to a server over time with this extension",
	"version": "0.1",
	"app": {
		"background": {
			"scripts": ["main.js"]
		}
	},
	"sandbox": {
		"pages": ["index.html"]
	},
	"icons": {
		"16": "ppp16.png",
		"32": "ppp32.png"
	},
	"permissions": ["<all_urls>"]
}